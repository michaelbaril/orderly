{
    "name": "baril/orderly",
    "description": "Orderable/sortable behavior for Eloquent models.",
    "keywords": [
        "laravel",
        "eloquent",
        "database",
        "sortable",
        "orderable"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Baril\\Orderly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baril\\Orderly\\Tests\\": "tests/",
            "Baril\\Orderly\\Tests\\Factories\\": "tests/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baril\\Orderly\\OrderlyServiceProvider"
            ]
        }
    }
}
